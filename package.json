{
    "name": "rrkallan-demo",
    "version": "0.1.0",
    "private": true,
    "author": {
        "name": "Ravi Rishaad Kallan",
        "email": "r.kallan@icloud.com",
        "url": "https://github.com/rkallan"
    },
    "scripts": {
        "preanalyze": "npm run build",
        "analyze": "source-map-explorer 'build/static/js/*.js{.map}'",
        "prebuild": "rm -rf ./build",
        "format": "npx prettier . --write",
        "format:check": "npx prettier .",
        "lint": "eslint . --fix",
        "lint:check": "eslint .",
        "npm-updates": "npx npm-check-updates",
        "npm-updates-u": "npx npm-check-updates -u",
        "preinstall": "/bin/bash ./scripts/pre-install.sh",
        "sort:package": "npx sort-package-json",
        "start": "(cd api && npm start) & react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "start:production": "npx serve -s build -p 1888 --ssl-cert ./.cert/cert.pem --ssl-key ./.cert/key.pem",
        "start:production-noSSL": "npx serve -s build -p 2888",
        "prestart:productionBuild": "npm run build",
        "start:productionBuild": "npx serve -s build -p 1888 --ssl-cert ./.cert/cert.pem --ssl-key ./.cert/key.pem",
        "prestart:productionBuild-noSSL": "npm run build",
        "start:productionBuild-noSSL": "npx serve -s build -p 2888",
        "prepare": "husky install"
    },
    "dependencies": {
        "@loadable/component": "^5.15.2",
        "@react-pdf/renderer": "^3.0.0",
        "@react-spring/web": "^9.5.5",
        "@reduxjs/toolkit": "^1.8.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "buffer": "^6.0.3",
        "graphql": "^16.6.0",
        "graphql.macro": "^1.4.2",
        "html-to-react": "^1.5.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.3.0",
        "react-redux": "^8.0.4",
        "react-router": "^6.4.2",
        "react-router-dom": "^6.4.2",
        "react-scripts": "^5.0.1",
        "redux": "^4.2.0",
        "reduxjs-toolkit-persist": "^7.2.1",
        "sass": "^1.55.0",
        "source-map-explorer": "^2.5.3",
        "typescript": "^4.8.4",
        "util": "^0.12.5",
        "web-vitals": "^3.0.4"
    },
    "devDependencies": {
        "@types/jest": "^29.2.0",
        "@types/loadable__component": "^5.13.4",
        "@types/node": "^18.11.5",
        "@types/react": "^18.0.22",
        "@types/react-dom": "^18.0.7",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "2.7.1"
    },
    "engines": {
        "node": ">=16.15.0",
        "npm": ">=8.0.0"
    }
}
